package com.DSA.logivalproblems;

public class DeciToBin {
	
	public static Long decToBin(int deci) {
		Long bin =0l;
		while(deci>0) {
			int res = deci % 2;
			System.out.println(res);
			bin = bin+(10*res);
			deci/=2;
		}
		return bin;
	}

	public static void main(String[] args) {
		System.out.println(decToBin(5));
	}
}
